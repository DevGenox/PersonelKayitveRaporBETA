        <Label x:Name="label1" Content="Verilecek Olan Miktar " HorizontalAlignment="Left" Margin="150,26,0,0" VerticalAlignment="Top" Width="131" Background="#FFEAC729"/>
        <DatePicker Name="Costomverilenzaman" HorizontalAlignment="Left" Margin="27,133,0,0" VerticalAlignment="Top"/>
        <xctk:TimePicker Name="saat" Margin="159,133,47,47"/>
        <Label x:Name="label2" Content="İsteğe Göre Tarih" HorizontalAlignment="Left" Margin="92,102,0,0" VerticalAlignment="Top" Width="114" Background="#FFEAC729"/>

    </Grid>
